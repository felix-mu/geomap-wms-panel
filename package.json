  "name": "felixrelleum-geomapwms-panel",
  "version": "2.0.1",
  "description": "",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "playwright test --trace on",
    "e2e:cypress-troubleshoot": "Xvfb :99 & export DISPLAY=:99 && npm exec cypress install && npx cypress run --browser electron --headless && pkill Xvfb",
    "e2e:cypress": "npm exec cypress install && npm exec grafana-e2e run",
    "e2e:cypress-update": "npm exec cypress install && npm exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache .",
    "lint:fix": "npm run lint -- --fix  && prettier --write --list-different .",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest --distDir ./felixrelleum-geomapwms-panel",
    "validate": "npm run build && zip -qr felixrelleum-geomapwms-panel.zip felixrelleum-geomapwms-panel && npx --yes @grafana/plugin-validator@latest -config ./plugin-validator-config.yml -sourceCodeUri file://. felixrelleum-geomapwms-panel.zip; rm -r felixrelleum-geomapwms-panel.zip",
    "test:all": "jest --coverage",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "commit": "cz",
    "release": "commit-and-tag-version"
  },
  "commit-and-tag-version": {
    "skip": {
      "bump": true,
      "commit": true,
      "tag": true
    }
  },
